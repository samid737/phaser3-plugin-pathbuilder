{
  "name": "phaser3-plugin-pathbuilder",
  "version": "1.8.0",
  "description": "Draw and edit Lines, Bezier Curves, Splines during runtime, explore your scene and export path tweens and PathFollowers",
  "main": "src/main.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "dist": "webpack --config webpack.dist.config.js",
    "lint": "eslint --config .eslintrc.json \"src/**/*.js\"",
    "lintfix": "eslint --config .eslintrc.json \"src/**/*.js\" --fix",
    "sloc": "node-sloc './src' --include-extensions \"js\"",
    "dev": "npm run build && webpack-dev-server --port=8000",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "curves",
    "Curve",
    "Editor",
    "editor",
    "build",
    "generate",
    "JSON",
    "export",
    "Phaser 3",
    "Phaser",
    "plugin",
    "pathFollowers",
    "pahts",
    "tweens",
    "animation",
    "motion",
    "drawing",
    "scene",
    "explore",
    "editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samid737/phaser3-plugin-pathbuilder"
  },
  "author": "samid737",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samid737/phaser3-plugin-pathbuilder/issues"
  },
  "homepage": "https://github.com/samid737/phaser3-plugin-pathbuilder#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "eslint": "^4.19.1",
    "fs-extra": "^5.0.0",
    "node-sloc": "^0.1.11",
    "raw-loader": "^0.5.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "uuid": "^3.3.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "snyk": "^1.316.2",
    "webpack-dev-server": "^3.10.3"
  },
  "snyk": true
}
